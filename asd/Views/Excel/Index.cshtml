@model List<asd.Models.Entities.SheetData>


    <form asp-controller="Excel" asp-action="Index" method="get">
        <p>Search: <input type="text" name="searchName"/>
            <input type="submit" name="Filter" /> </p>

    </form>
<table class="table table-dark table-hover">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Middle Name</th>
        <th>Room</th>
        <th>Dormitory</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.firstName</td>
            <td>@item.lastName</td>
            <td>@item.midleName</td>
            <td>@item.room</td>
            <td>@item.dormintory</td>
            <td>
               @*  <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a> *@
            </td>
        </tr>
    }
</table>
